  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["dev"],
      "skipFiles": ["<node_internals>/**"],
      "env": {
        "MOCKS_ENABLED": "true",
        "GH_APP_ID": "1",
        "GH_PRIVATE_KEY": "",
        "GH_APP_INSTALLATION_ID": "1234"
      },
      "outFiles": ["${workspaceFolder}/**/*.js"]
    }
  ]
}
